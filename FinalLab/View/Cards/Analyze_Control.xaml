<UserControl x:Class="FinalLab.View.Cards.Analyze_Control"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:FinalLab.View.Cards"
             mc:Ignorable="d" 
             d:DesignHeight="63" d:DesignWidth="349"
             Background="{DynamicResource PrimaryBackground}">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="2*"/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <TextBlock FontFamily="{StaticResource Inter}" FontSize="14" TextWrapping="Wrap" TextAlignment="Center" VerticalAlignment="Center" Grid.Row="0">Определение антигена Coronavirus (SARS-CoV-2) качественно</TextBlock>
        <TextBlock FontFamily="{StaticResource Inter}" FontSize="12" TextAlignment="Center" VerticalAlignment="Center" Foreground="{DynamicResource SecondaryForeground}" Grid.Row="1">дата дата я пидор</TextBlock>
    </Grid>
</UserControl>
